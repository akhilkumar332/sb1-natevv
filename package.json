{
  "name": "blood-donation-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node src/functions/index.js",
    "dev:server": "nodemon src/functions/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "deploy": "npm run build:prod && firebase deploy",
    "deploy:hosting": "npm run build:prod && firebase deploy --only hosting",
    "deploy:rules": "firebase deploy --only firestore:rules,storage:rules",
    "deploy:indexes": "firebase deploy --only firestore:indexes",
    "firebase:emulators": "firebase emulators:start",
    "firebase:login": "firebase login",
    "firebase:init": "firebase init"
  },
  "dependencies": {
    "@react-three/drei": "^9.116.2",
    "@react-three/fiber": "^8.17.10",
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.7.7",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.1.0",
    "i18next": "^23.16.4",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^15.1.0",
    "react-leaflet": "^4.2.1",
    "react-phone-number-input": "^3.4.12",
    "react-router-dom": "^6.27.0",
    "react-slick": "^0.30.2",
    "slick-carousel": "^1.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/fs-extra": "^11.0.1",
    "@types/google.maps": "^3.58.1",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-phone-number-input": "^3.0.17",
    "@types/react-slick": "^0.23.13",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "fs-extra": "^11.2.0",
    "globals": "^15.9.0",
    "jsdom": "^27.0.0",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
